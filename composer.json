{
    "name": "facturama/facturama-php-sdk",
    "description": "Facturama PHP SDK",
    "keywords": [
        "API",
        "PHP",
        "JSON",
        "Facturama"
    ],
    "license": "MIT",
    "type": "library",
    "homepage": "https://www.api.facturama.com.mx/",
    "support": {
        "issues": "https://github.com/Facturama/facturama-php/issues"
    },
    "authors": [
        {
            "name": "Facturama",
            "email": "dev@facturama.com"
        }
    ],
    "require": {
        "php": ">=7.4 <9.0",
        "ext-curl": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.8.2"
    },
    "conflict": {
        "guzzlehttp/guzzle": "<7.8.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^11.0",
        "phpmd/phpmd": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "Facturama\\": [
                "src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Facturama\\Tests\\": "tests/"
        },
        "files": [
            "examples/credentials.php"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    }
}